LIB = @buildlib@/libctmath.a

.SUFFIXES :
.SUFFIXES : .f .d .o .cpp

F_FLAGS = @FFLAGS@ @F77FLAGS@

OBJS = \
mach.o \
ddaspk.o \
dgbefa.o \
dgbsl.o \
dgefa.o \
dgesl.o \
dp1vlu.o \
dpcoef.o \
dpolft.o \
fdump.o \
j4save.o \
pcoef.o \
polfit.o \
pvalue.o \
xercnt.o \
xerhlt.o \
xermsg.o \
xerprn.o \
xersve.o \
xgetua.o 

SRCS = $(OBJS:.o=.cpp)
all = $(LIB)

$(LIB):  $(OBJS)
	@ARCHIVE@ $(LIB) $(OBJS) > /dev/null

%.o : %.cpp
	@CXX@ -c $< @DEFS@  @CXXFLAGS@ $(INCLUDES)

%.o : %.f
	@F77@ -c $< $(F_FLAGS)

clean:
	$(RM) $(OBJS) $(LIB)

depends:
	echo '...'

 



