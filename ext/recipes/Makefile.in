#/bin/sh

LIB = @buildlib@/librecipes.a

SUFFIXES=
SUFFIXES= .f .d .o

F_FLAGS = @FFLAGS@ @F77FLAGS@

OBJS = simp1.o simp2.o simp3.o simplx.o 
SRCS = $(OBJS:.o=.cpp)


all:  $(LIB)

$(LIB): $(OBJS)
	@ARCHIVE@ $(LIB) $(OBJS) > /dev/null

%.o : %.f
	@F77@ -c $< $(F_FLAGS)

clean:
	$(RM) $(OBJS) $(LIB)

depends: 



