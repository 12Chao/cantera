all: refresh
	@PYTHON_CMD@ setup.py build

refresh: $(LIB_DEPS)
	touch src/pycantera.cpp

install:
	@PYTHON_CMD@ setup.py install

clean:
	@PYTHON_CMD@ setup.py clean
	cd src; rm -f *.o

depends:
	echo '-'

