#!/bin/sh

PY_DEMOS = critProperties rankine

all:
	@(for py in $(PY_DEMOS) ; do \
	    echo "running $${py}..."; \
	  (cd $${py} ; @MAKE@ ) \
	done)

run:
	@(for py in $(PY_DEMOS) ; do \
	    echo "running $${py}..."; \
	  (cd $${py} ; @MAKE@ run ) \
	done)

test:
	@(for py in $(PY_DEMOS) ; do \
	    echo "running $${py}..."; \
	  (cd $${py} ; @MAKE@ test) \
	done)

clean:
	@(for py in $(PY_DEMOS) ; do \
	    echo "running $${py}..."; \
	  (cd $${py} ; @MAKE@ clean) \
	done)



